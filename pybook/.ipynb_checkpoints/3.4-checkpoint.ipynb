   "source": [
    "def sol(n, m):\n",
    "    count = 0\n",
    "    while(n != 1):\n",
    "        while(n % m != 0):\n",
    "            if(n == 1):\n",
    "                break\n",
    "            n -= 1\n",
    "            count += 1\n",
    "        n /= m;\n",
    "        count += 1\n",
    "    print(count)"
   ]
