    "def sol(n):\n",
    "    slist = []\n",
    "    for i in range(n):\n",
    "        name, grade = input().split()\n",
    "        grade = int(grade)\n",
    "        slist.append([name, grade])\n",
    "    slist.sort(key = lambda x:x[1])\n",
    "    \n",
    "    for i in slist:\n",
    "        print(i[0], end = ' ')"
   ]
